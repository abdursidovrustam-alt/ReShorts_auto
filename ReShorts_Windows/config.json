{
  "search": {
    "queries": [
      "#viral",
      "#trending",
      "funny videos",
      "популярное",
      "short videos",
      "entertainment"
    ],
    "max_results": 20,
    "date_range": "this_week",
    "min_views": 1000,
    "max_views": 10000000,
    "min_likes": 50,
    "min_engagement": 2.0,
    "duration_min": 0,
    "duration_max": 300,
    "exclude_keywords": ["spam", "ads", "реклама", "18+"]
  },
  "ai": {
    "providers": {
      "gpt4free": {
        "enabled": true,
        "priority": 1,
        "timeout": 30
      },
      "gemini": {
        "enabled": true,
        "priority": 2,
        "timeout": 25
      },
      "openrouter": {
        "enabled": false,
        "priority": 3,
        "model": "deepseek/deepseek-chat",
        "api_key": "",
        "timeout": 20
      },
      "localai": {
        "enabled": false,
        "priority": 4,
        "url": "http://localhost:8080",
        "model": "gpt-3.5-turbo",
        "timeout": 15
      }
    },
    "timeout": 30,
    "max_retries": 3,
    "cache_enabled": true,
    "default_prompt": "Проанализируй это видео и объясни, почему оно может быть вирусным. Сосредоточься на контенте, заголовке, статистике и трендах."
  },
  "download": {
    "methods": {
      "ytdlp": {
        "enabled": true,
        "priority": 1,
        "timeout": 120
      },
      "requests": {
        "enabled": true,
        "priority": 2,
        "timeout": 60
      }
    },
    "video_quality": "best[height<=720]",
    "max_file_size": 100,
    "max_retries": 3,
    "path": "downloads",
    "subtitles": false,
    "thumbnail": true,
    "proxy": {
      "enabled": false,
      "url": ""
    }
  },
  "processing": {
    "clip_duration": 30,
    "clips_per_video": 3,
    "output_format": "mp4",
    "video_bitrate": 2500,
    "auto_crop": true,
    "add_watermark": false,
    "filter_duplicates": true,
    "filter_low_quality": true,
    "output_path": "processed"
  },
  "automation": {
    "auto_search": false,
    "auto_download": false,
    "auto_analyze": false,
    "auto_process": false,
    "auto_run_interval": 60,
    "notify_on_complete": true,
    "notify_on_error": true,
    "max_daily_downloads": 50
  },
  "paths": {
    "downloads": "downloads",
    "processed": "processed", 
    "logs": "logs",
    "tmp": "tmp",
    "output": "output"
  }
}